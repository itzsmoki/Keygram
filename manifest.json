{
    "manifest_version": 3,
    "name": "Keygram",
    "version": "1.0.0",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["https://www.instagram.com/*"],
    "action": {
        "default_popup": "menu.html"
      },
    "content_scripts": [
        {
            "matches": ["https://www.instagram.com/*"],
            "js": ["content.js"]
        }
    ]
}